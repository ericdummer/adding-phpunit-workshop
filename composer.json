{
  "name": "ericdummer/docker-php-composer",
  "description": "template repo to create a php project",
  "authors": [
    {
      "name": "Eric Dummer"
    }
  ],
  "require": {},
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
